{% extends 'base.html.twig' %}

{% block title %}EquipmentInt index{% endblock %}

{% block body %}
    <h1>EquipmentInt index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for equipment_int in equipment_ints %}
            <tr>
                <td>{{ equipment_int.id }}</td>
                <td>{{ equipment_int.name }}</td>
                <td>
                    <a href="{{ path('app_equipment_int_show', {'id': equipment_int.id}) }}">show</a>
                    <a href="{{ path('app_equipment_int_edit', {'id': equipment_int.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_equipment_int_new') }}">Create new</a>
{% endblock %}
